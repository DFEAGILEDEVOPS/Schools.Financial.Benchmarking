@{
    ViewBag.assetsPath = "/public/govuk_template/assets";

    var appTitle = "Schools financial benchmarking";
    var documentTitle = !string.IsNullOrEmpty(ViewBag.Title) ?
        string.Format("{0}{1} - Schools Financial Benchmarking - GOV.UK", ViewBag.ErrorPrefix?? "", ViewBag.Title) :
        string.Format("{0}Schools Financial Benchmarking - GOV.UK", ViewBag.ErrorPrefix?? "");
}
<!DOCTYPE html>
<!--[if lt IE 9]><html class="lte-ie8" lang="en"><![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en">
<!--<![endif]-->
<head prefix="og: http://ogp.me/ns#">
    <meta charset="utf-8" />
    <title>@documentTitle</title>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="@(ViewBag.assetsPath)/images/opengraph-image.png?0.12.0">
    <meta name="theme-color" content="#0b0c0c" />
    <meta http-equiv="Content-Security-Policy" content="script-src 'self' 'unsafe-eval' 'unsafe-inline' www.googletagmanager.com www.google-analytics.com atlas.microsoft.com ">
    <meta http-equiv="Content-Security-Policy" content="object-src 'none'">
    <meta http-equiv="Content-Security-Policy" content="frame-src 'none'">
    <meta http-equiv="Content-Security-Policy" content="media-src 'none'">
    <meta http-equiv="Content-Security-Policy" content="connect-src 'self' www.google-analytics.com api.postcodes.io atlas.microsoft.com www.compare-school-performance.service.gov.uk @System.Configuration.ConfigurationManager.AppSettings["SfbApiUrl"]">
    <meta http-equiv="Content-Security-Policy" content="font-src 'self' data:">
    <meta http-equiv="Content-Security-Policy" content="manifest-src 'self'">
    <meta http-equiv="Content-Security-Policy" content="style-src 'self' 'unsafe-inline'">
    <meta http-equiv="Content-Security-Policy" content="img-src 'self' www.googletagmanager.com www.google-analytics.com atlas.microsoft.com data:">
    <meta http-equiv="Content-Security-Policy" content="form-action 'self'">
    <link rel="shortcut icon" href="/public/assets/images/icons/favicon.ico" type="image/x-icon" />
    <link rel="mask-icon" href="/public/assets/images/icons/gov.uk_logotype_crown.svg" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="180x180" href="~/public/assets/images/icons/apple-touch-icon-180x180.png">
    <link rel="apple-touch-icon" sizes="167x167" href="~/public/assets/images/icons/apple-touch-icon-167x167.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/public/assets/images/icons/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/public/govuk_template/assets/images/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" href="/public/assets/images/icons/apple-touch-icon.png">

    @*//////// Added by SFB dev in regards to story #18856 ////////////////*@
    @if (ViewBag.pageDescription == null)
    {
        <meta name="robots" content="noindex">
    }
    else
    {
        <meta name="description" content="@(ViewBag.pageDescription)">
    }
    @*//////// Added by SFB dev in regards to story #18856 ////////////////*@

    <script type="text/javascript">
        (function () {
            if (navigator.userAgent.match(/IEMobile\/10\.0/)) {
                var d = document, c = "appendChild", a = d.createElement("style");
                a[c](d.createTextNode("@@-ms-viewport{width:auto!important}"));
                d.getElementsByTagName("head")[0][c](a);
            }
        })();
    </script>

    <!--[if gt IE 8]><!-->
    <link href="@(ViewBag.assetsPath)/stylesheets/govuk-template.css?0.13.0" media="screen" rel="stylesheet" type="text/css" /><!--<![endif]-->
    <!--[if IE 8]><link href="@(ViewBag.assetsPath)/stylesheets/govuk-template-ie8.css?0.12.0" media="screen" rel="stylesheet" type="text/css"/><![endif]-->

    <link href="@(ViewBag.assetsPath)/stylesheets/govuk-template-print.css?0.13.0" media="print" rel="stylesheet" type="text/css" />

    <!--[if lt IE 9]>
        <script src="@(ViewBag.assetsPath)/javascripts/ie.js?0.12.0" type="text/javascript"></script>
    <![endif]-->

    @RenderSection("Head", required: false)
</head>
<body class="@(ViewBag.bodyClasses)" onbeforeunload="DfE.Util.clearDropdowns(event)">
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=@System.Configuration.ConfigurationManager.AppSettings["GoogleTagManagerId"]"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <a name="pagetop"></a>
    @if (!Request.Browser.Browser.Contains("MSIE 8"))
    {
        <script type="text/javascript">
            document.body.className =
                ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
    }

    @RenderSection("bodyStart", required: false)

    @if (Request.Path.Contains("/Help"))
    {
        <div id="skiplink-container">
            <div>
                <a href="#helpContent" class="skiplink">Skip to main content</a>
            </div>
        </div>
    }
    else
    {
        <div id="skiplink-container">
            <div>
                <a href="#content" class="skiplink">Skip to main content</a>
            </div>
        </div>
    }

    <div id="global-cookie-message" class="gem-c-cookie-banner govuk-clearfix" role="region" aria-label="cookie banner" data-nosnippet="" style="display: none;">
        <div class="gem-c-cookie-banner__wrapper govuk-width-container">
            <div class="grid-row">
                <div class="column-two-thirds">
                    <div class="gem-c-cookie-banner__message">
                        <span class="cookie-heading-m govuk-heading-m">Tell us whether you accept cookies</span>
                        <p class="govuk-body">We use <a class="govuk-link" href="/help/cookies">cookies to collect information</a> about how you use School Financial Benchmarking. We use this information to make the website work as well as possible and improve government services.</p>
                    </div>
                    <div class="gem-c-cookie-banner__buttons">
                        <div class="gem-c-cookie-banner__button gem-c-cookie-banner__button-accept column-half">
                            <button id="acceptAllCookies" class="button gem-c-button govuk-button gem-c-button--inline" type="submit">Accept all cookies</button>
                        </div>
                        <div class="gem-c-cookie-banner__button gem-c-cookie-banner__button-settings column-half">
                            <a class="button gem-c-button govuk-button gem-c-button--inline" role="button" href="/help/cookies">Set cookie preferences</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="gem-c-cookie-banner__confirmation govuk-width-container" tabindex="-1" style="display: none">
            <p class="gem-c-cookie-banner__confirmation-message">
                You’ve accepted all cookies. You can <a class="govuk-link" href="/help/cookies" data-module="track-click" data-track-category="cookieBanner" data-track-action="Cookie banner settings clicked from confirmation">change your cookie settings</a> at any time.
            </p>
            <button id="acceptAllCookiesHide" class="link-button gem-c-cookie-banner__hide-button">Hide</button>
        </div>
    </div>

    <div id="cookie-overlay-wrapper">
        <header role="banner" id="global-header" class="with-proposition" aria-labelledby="proposition-name">
            <div class="header-wrapper">
                <div class="header-global">
                    <div class="header-logo">
                        <a href="https://www.gov.uk/" title="Go to the GOV.UK homepage" id="logo" class="content">
                            <img src="@(ViewBag.assetsPath)/images/gov.uk_logotype_crown_invert_trans.png" width="35" height="31" alt=""> GOV.UK
                        </a>
                    </div>
                </div>
                <div class="header-proposition">
                    <div class="content">
                        @*<a href="#proposition-links" class="js-header-toggle menu">Menu</a>*@
                        <div id="proposition-menu">
                            <a href="/" id="proposition-name">@appTitle</a>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <header id="banner-header" class="banner-content__recruitment-banner" aria-label="Help develop Schools financial benchmarking banner" style="display: none">
            <div class="header-wrapper">
                <div class="header-wrapper__header-proposition">
                    <span class="bold-small">Help develop Schools financial benchmarking</span>
                    <a href="/help/get-involved" rel="external noopener noreferrer" target="_blank" title="Get involved form will open in a new page">Get involved in making this service better (opens in new tab or window)</a>
                </div>
                <div class="header-wrapper__hide-button-wrapper">
                    <button class="link-button js-dismiss-recruitment-banner" href="">No thanks<span class="visually-hidden">: I do not want to help improving the service</span></button>
                </div>
            </div>
        </header>

        <div id="global-header-bar"></div>
        <!--end header-->

        @RenderSection("AfterHeader", required: false)

        <div class="global-header-bar">
        </div>
        <!--end global-header-bar-->

        <div class="side-width-wrapper hide-in-print">
            <div class="grid-row">
                <div class="column-full">
                    <div class="phase-banner">
                        @(Html.Partial("../govuk/partials/phase/beta"))
                    </div>
                </div>
            </div>
        </div>

        @*<div class="side-width-wrapper">
            <div class="grid-row">
                <div class="column-full">
                    <img class="top-logo" src="/public/assets/images/dfe-xsmall.png" />
                </div>
            </div>
        </div>*@

        @RenderBody()

        <div class="global-header-bar">
        </div>
        <footer class="group js-footer" id="footer" role="contentinfo">
            <div class="footer-wrapper">
                <div class="footer-meta">
                    <div class="footer-meta-inner">
                        <h2 class="visuallyhidden">Support links</h2>
                        <ul>
                            <li>
                                <a href="/help/guidance">Help</a>
                            </li>
                            <li>
                                <a href="/help/cookies">Cookies</a>
                            </li>
                            <li>
                                <a href="/accessibility">Accessibility</a>
                            </li>
                            <li>
                                <a target="_blank" rel="noopener noreferrer" href="https://form.education.gov.uk/fillform.php?self=1&form_id=cCCNJ1xSfBE&noLoginPrompt=1">Contact<span class="visuallyhidden"> Opens in a new window</span></a>
                            </li>
                            <li>
                                <a href="/help/get-involved" rel="external noopener noreferrer" target="_blank" title="Get involved form will open in a new page">Help with usability testing</a>
                            </li>
                        </ul>

                        <div class="open-government-licence">
                            <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>

                            <p>All content is available under the <a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated</p>

                        </div>
                    </div>
                    <div class="copyright">
                        <a href="https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">© Crown copyright</a>
                    </div>
                </div>
            </div>
        </footer>
        <!--end footer-->

        <div id="global-app-error" class="app-error hidden"></div>
        <script src="@(ViewBag.assetsPath)/javascripts/govuk-template.js?0.13.0" type="text/javascript"></script>

        @RenderSection("BodyEnd", required: false)
    </div>
</body>

</html>